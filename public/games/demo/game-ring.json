{
  "className": "GameRing",
  "world": {
    "backwall": {
      "anim": "backwall",
      "size": ["{viewportWidth}", "{viewportHeight}"],
      "opacity": 0.5,
      "z": -1000
    },
    "mazoo": {
      "repeat": 1000,
      "anim": "mazoo_still",
      "size": [16, 16],
      "x": "{random * viewportWidth}", "y": "{random * viewportHeight}",
      "z": "{random * viewportHeight / 400 * 100}",
      "list": "mazoos",
      "properties": {
        "goal": [
          "{random * viewportWidth}",
          "{random * viewportHeight}"
        ]
      }
    }
  },
  "atlas": {
    "backwall": {
      "url": "assets/backwall.jpg",
      "hotspot": [0, 0]
    },
    "mazoo_still": {
      "url": "assets/mazoo.png",
      "cols": 3, "rows": 4,
      "range":[0],
      "hotspot": "{hotspot_bottom}"
    },
    "mazoo_down": {
      "url": "assets/mazoo.png",
      "cols": 3, "rows": 4,
      "frameRate": 10,
      "range":[0, 3],
      "hotspot": "{hotspot_bottom}"
    },
    "mazoo_up": {
      "url": "assets/mazoo.png",
      "cols": 3, "rows": 4,
      "frameRate": 10,
      "range":[4, 7],
      "hotspot": "{hotspot_bottom}"
    },
    "mazoo_right": {
      "url": "assets/mazoo.png",
      "cols": 3, "rows": 4,
      "frameRate": 10,
      "range":[8, 11],
      "hotspot": "{hotspot_bottom}"
    },
    "mazoo_left": {
      "url": "assets/mazoo.png",
      "cols": 3, "rows": 4,
      "range":[8, 11],
      "frameRate": 10,
      "direction": -1,
      "hotspot": "{hotspot_bottom}"
    }
  }
}